        "api.PlanRequest": {
            "description": "Request para gerar plano de execução",
            "type": "object",
            "properties": {
                "resources": {
                    "description": "Lista de recursos AWS para planejar\nExample: [{\"apiVersion\":\"aws-infra-operator.runner.codes/v1alpha1\",\"kind\":\"VPC\",\"metadata\":{\"name\":\"my-vpc\"},\"spec\":{\"cidrBlock\":\"10.0.0.0/16\"}}]",
